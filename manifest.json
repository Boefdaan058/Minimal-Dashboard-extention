{
  "manifest_version": 3,
  "name": "Minimal Dashboard",
  "version": "1.0",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "homepage.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "128": "icons/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/*", "libs/anime.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
